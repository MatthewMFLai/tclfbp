name dictionary 
filepath $env(COMP_HOME)/bigEnglish/dictionary
# port format
# <port type> {portname msgdef} {portname msgdef} ... 
inports {1 $env(COMP_HOME)/msgdef/bigenglish/dictionary.msg} {2 $env(COMP_HOME)/msgdef/bigenglish/dictionary_set.msg}
outports {1 $env(COMP_HOME)/msgdef/bigenglish/dictionary.msg} {2 $env(COMP_HOME)/msgdef/bigenglish/dictionary_resp.msg}
