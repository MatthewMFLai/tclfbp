source sendfile.tcl
send_one_file $env(HOME)/tclfbp/component/2.0/testall/test1/$id.node localhost 14001
send_one_file $env(HOME)/tclfbp/component/2.0/testall/test1/$id.link localhost 14001

set sd [socket localhost 14000]

puts $sd "$id CREATE"
flush $sd
gets $sd rc
puts "rc = $rc"

puts $sd "$id INIT"
flush $sd
gets $sd rc
puts "rc = $rc"

puts $sd "$id IDENT"
flush $sd
gets $sd rc
puts "rc = $rc"

puts $sd "$id ENABLE"
flush $sd
gets $sd rc
puts "rc = $rc"

